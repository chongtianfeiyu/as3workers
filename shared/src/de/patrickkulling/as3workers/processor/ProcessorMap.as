package de.patrickkulling.as3workers.processor{	import flash.utils.Dictionary;	public class ProcessorMap	{		private static const EMPTY_PROCESSOR:Processor = new EmptyProcessor();		private var processorMap:Dictionary;		public function ProcessorMap()		{			processorMap = new Dictionary();		}		public function map(messageID:int, processor:Processor):void		{			processorMap[messageID] = processor;		}		public function getProcessor(messageID:int):Processor		{			var processor:Processor = processorMap[messageID];			if(processor == null)			{				trace("mapping to Command for id:", messageID, "not found.");				processor = EMPTY_PROCESSOR;			}			return processor;		}	}}